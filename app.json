{
  "expo": {
    "name": "Guard Imoto",
    "slug": "guard-imoto-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/android/mipmap-hdpi/ic_launcher.png",
    "scheme": "guardimotoproject",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kenshibu.guard-imoto-project"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/icons/main-logo.png"
      },
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCKWbmWOGaNIn29P2EIOJRrQdZyb6pTIyI"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kenshibu.guardimotoproject"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icons/main-logo.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "custom photos permission",
          "cameraPermission": "Allow $(PRODUCT_NAME) to open the camera",
          "//": "Disables the microphone permission",
          "microphonePermission": false
        }
      ],
      "expo-router",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/main-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/icons/main-logo.png",
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c6716259-fe4e-4fd0-ac1e-b713878d1a63"
      }
    }
  }
}
