{
  "expo": {
    "name": "Guard Imoto Project",
    "slug": "guard-imoto-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/android/mipmap-hdpi/ic_launcher.png",
    "scheme": "guardimotoproject",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kenshibu.guard-imoto-project"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/icons/main-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kenshibu.guardimotoproject"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icons/main-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/main-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/icons/main-logo.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
